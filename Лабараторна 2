 #include <iostream>
 
int main() {
    int sum = 0;
 
    // Обчислюємо суму чисел від 1 до 100
    for (int i = 1; i <= 100; ++i) {
        sum += i;
    }
 
    // Виводимо результат
    std::cout << "Сума чисел від 1 до 100: " << sum << std::endl;
 
    return 0;
} 
